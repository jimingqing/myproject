package handout;

public abstract class Task implements Runnable {
    public long _taskID = 0;
    public Task(){
        
    }
    
    public Task(long task_id){
        _taskID = task_id;
    }
    
    public void setTaskId(long task_id){
        _taskID = task_id;
    }
    
    public long getTaskId(){
        return _taskID;
    }
    
    public void run(){
        System.out.printf("test\r\n");
    }
}
